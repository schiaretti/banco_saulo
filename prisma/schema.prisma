generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuario {
 id      Int     @id @default(autoincrement())
  email   String   @unique
  nome    String
  senha   String
 
}

model cliente {
  id      Int     @id @default(autoincrement())
  email     String    @unique
  nome    String
  cnpj     String
  celular String    
  contato String
}

model logistica {
  id      Int     @id @default(autoincrement())
  usuario     String    
  cliente    String
  cidorigem     String
  ciddestino String    
  freteemp String
  fretemot String
  produto String
  veiculo String
}
